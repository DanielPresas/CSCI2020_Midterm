<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.shape.*?>
<?import javafx.geometry.Insets?>

<VBox alignment="center" spacing="40.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.danielpresas_100699431.AnimationController">
  <children>
    <Label text="Animation" />
    <HBox alignment="center_left">
      <Rectangle fx:id="rectangle" x="0" y="0" width="250" height="150" fill="#ff0000" />
    </HBox>
    <Button fx:id="backButton" text="Back to main menu" onAction="#backToMain" />
  </children>
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
</VBox>
